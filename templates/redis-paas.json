{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "cachename": {
            "type": "String"
        },
        "location": {
            "type": "String"
        },
        "subnetid": {
            "type": "String"
        },
        "env": {
            "type": "string",
            "metadata": {
                "description": "The tag"
            }
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Cache/Redis",
            "name": "[parameters('cachename')]",
            "apiVersion": "2016-04-01",
            "location": "[parameters('location')]",
            "tags": {
                "displayName": "Redis Cache",
                "environment": "[parameters('env')]"
            },
            "scale": null,
            "properties": {
                "redisVersion": "3.2.7",
                "sku": {
                    "name": "Premium",
                    "family": "P",
                    "capacity": 1
                },
                "enableNonSslPort": false,
                "redisConfiguration": {
                    "maxclients": "7500",
                    "maxmemory-reserved": "200",
                    "maxfragmentationmemory-reserved": "300",
                    "maxmemory-delta": "200"
                },
                "subnetId": "[parameters('subnetid')]"
            },
            "dependsOn": []
        }
    ],
    "outputs": {
        "accessKey": {
            "type": "string",
            "value": "[listKeys(resourceId('Microsoft.Cache/Redis', parameters('cacheName')), '2015-08-01').primaryKey]"
        },
        "hostName": {
            "type": "string",
            "value": "[reference(resourceId('Microsoft.Cache/Redis', parameters('cacheName')), '2015-08-01').hostName]"
        },
        "sslPort": {
            "type": "int",
            "value": "[reference(resourceId('Microsoft.Cache/Redis', parameters('cacheName')), '2015-08-01').sslPort]"
        }
      }
}